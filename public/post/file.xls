var focusedElBeforeOpen;

function setFocusedElBeforeOpen() {
    focusedElBeforeOpen = document.activeElement; // Store the element that opened the modal before redirecting focus
}

function setFocusOnModalClose() {
    focusedElBeforeOpen.focus();
    $('#import').off('keydown');
}

function containImportModalFocus() {
    var focusableElements = $('.js-focusable-modal-element'); // Get a list of focusable modal elements
    focusableElements = Array.prototype.slice.call(focusableElements);
    // focusableElements.first().focus(); // Set focus to the first focusable element

    $(this).on('keydown', function(e) {
        var key = e.which;
        var TAB_KEY = 9;
        var ENTER_KEY = 13;
        if ( key == TAB_KEY ) {
            for ( var i = 0; i < focusableElements.length; i++ ) {
                if ( document.activeElement == focusableElements[i] ) {
                    if ( e.shiftKey ) {
                        if ( i > 0 ) {
                            i--;
                        } else {
                            i = focusableElements.length - 1; 
                        }
                    } else {
                        if ( i < focusableElements.length - 1 ) {
                            i++;
                        } else {
                            i = 0;
                        }
                    }
                    e.preventDefault(); // Prevents tab key from sending focus outside of the modal
                    focusableElements[i].focus();
                }
            }
        }

        if ( $(e.target).hasClass('js-close-modal-element') && key == ENTER_KEY) {
            $('#import').modal('hide');
        }
    });
}

