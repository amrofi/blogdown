<script type="text/javascript">

    $(document).ready(function() {
        $('.wsis-scoring-select').change(function(e) {
            if ($(this).val().indexOf('scoring=custom') !== -1) {
                var league_id = null;
                if ($('.select-links[name ="mpb-league"]').val()) {
                    league_id = $('.select-links[name ="mpb-league"]').val().replace('?key=', '');
                }
                var isPremium = ("sub_level" in window._fp_user && window._fp_user.sub_level != '' && window._fp_user.sub_level != 'basic');
                if (league_id !== null && (league_id === undefined || league_id.indexOf('nfl~') !== 0)) {
                    league_id = null;
                }

                if (league_id === null && isPremium) {
                    alert("To see custom rankings you must first import or select a league.");
                } else if (isPremium) {
                    window.location = window.location.pathname + "?key=" + league_id + "&scoring=custom";
                } else {
                    $('#custom-scoring-modal').modal();
                    ga('send', 'event', 'NFL WSIS Custom Scoring', 'Custom Scoring Modal View');
                }

                e.preventDefault();
                e.stopImmediatePropagation();
                return false;
            }
        });

        // // reselect players from list to match those in the search inputs
        // var fast_id_elems = $('#fast-search input[name="filter[]"]');
        // var fastLen = fast_id_elems.length;
        // for(var i = 0; i < fastLen; ++i) {
        // 	var q_id = fast_id_elems[i].value;
        // 	if (q_id.length && q_id != ",") {
        // 		$('#'+q_id+'.player-select').addClass('player-selected');
        // 	}
        // }
        //

        //Bind the autocompletes
        for (var i = 1; i <= 2; i++) {
            $('#fast' + i).autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/ajax/players.php',
                        data: {
                            q: request.term,
                            index: 'nfl_players',
                            sport: 'NFL'
                        },
                        dataType: 'jsonp',
                        cache: true,
                        jsonpCallback: 'searchcb',
                        contentType: "application/json",
                        success: function (data) {
                            var results = [];
                            data.results.forEach(function (result) {
                                //Filter out players already selected
                                var playerSelected = false;
                                $('.fast-player').each(function () {
                                    if ($(this).val() == result.player_id) {
                                        playerSelected = true;
                                    }
                                });

                                if (playerSelected) {
                                    return;
                                }
                                results.push(result);
                            });
                            response(results);
                        }
                    })
                },
                select: function (event, ui) {
                    $(this).attr('value', ui.item.name);
                    $('#id' + $(this).attr('id').replace('#', '')).attr('value', ui.item.player_id);
                    if ($('#' + ui.item.player_id).length) {
                        $('#' + ui.item.player_id).addClass('player-selected');
                    }
                    event.preventDefault();
                },
                focus: function (event, ui) {
                    this.value = ui.item.name;
                    event.preventDefault();
                }
            }).data('ui-autocomplete')._renderItem = function (ul, item) {
                return $('<li>')
                    .data('item.autocomplete', item)
                    .attr('data-pid', item.player_id + ':' + ((typeof item.ecr === 'undefined') ? 'NR' : item.ecr))
                    .append('<a>' + item.name + ' (' + item.eligibility + ' - ' + item.team + ')</a>')
                    .appendTo(ul);
            };
        }

        $('.wsis-box').click(function () {
            var id = $(this).attr('id');
            $(this).val('');
            $('#fast' + id).val('');
            if ($('#id' + id).val() != '') {
                var pid = $('#id' + id).val();
                $('#id' + id).val('');
                if ($('#' + pid).hasClass('player-selected')) {
                    $('#' + pid).removeClass('player-selected')
                }
            }
        });

        $('.player-select').click(function () {
            var sid = $(this).attr('id');
            var sname = $(this).find('a').attr('title');
            if ($(this).hasClass('player-selected')) {
                $(this).removeClass('player-selected');
                for (n = 1; n <= 2; n++) {
                    if ($('#idfast' + n).val() == sid) {
                        $('#idfast' + n).val('');
                        $('#fast' + n).val('');
                        break;
                    }
                }
                if ($(this).hasClass('QB') || $(this).hasClass('RB') || $(this).hasClass('WR') || $(this).hasClass('TE')) {
                    if ($('.player-selected').length == 0) {
                        $('.DST, .K, .IDP').addClass('player-select').removeClass('player-disabled');
                    }
                }
                if ($(this).hasClass('DST')) {
                    if ($('.player-selected').length == 0) {
                        $('.QB, .RB, .WR, .TE, .K, .IDP').addClass('player-select').removeClass('player-disabled');
                    }
                }
                if ($(this).hasClass('K')) {
                    if ($('.player-selected').length == 0) {
                        $('.QB, .RB, .WR, .TE, .DST, .IDP').addClass('player-select').removeClass('player-disabled');
                    }
                }
                if ($(this).hasClass('IDP')) {
                    if ($('.player-selected').length == 0) {
                        $('.QB, .RB, .WR, .TE, .DST, .K').addClass('player-select').removeClass('player-disabled');
                    }
                }
            } else {
                $(this).addClass('player-selected');
                for (n = 1; n <= 2; n++) {
                    if ($('#idfast' + n).val() == '') {
                        $('#idfast' + n).val(sid);
                        $('#fast' + n).val(sname);
                        break;
                    }
                    if (n == 2) {
                        var fid = $('#idfast2').val();
                        $('#idfast2').val(sid);
                        $('#fast2').val(sname);
                        $('#' + fid).removeClass('player-selected');
                    }
                }
                if ($(this).hasClass('QB') || $(this).hasClass('RB') || $(this).hasClass('WR') || $(this).hasClass('TE')) {
                    $('.DST, .K, .IDP').removeClass('player-select').addClass('player-disabled');
                }
                if ($(this).hasClass('DST')) {
                    $('.QB, .RB, .WR, .TE, .K, .IDP').removeClass('player-select').addClass('player-disabled');
                }
                if ($(this).hasClass('K')) {
                    $('.QB, .RB, .WR, .TE, .DST, .IDP').removeClass('player-select').addClass('player-disabled');
                }
                if ($(this).hasClass('IDP')) {
                    $('.QB, .RB, .WR, .TE, .DST, .K').removeClass('player-select').addClass('player-disabled');
                }
            }
        });

                $('#fast3, #fast4').removeClass('wsis-box').addClass('input-disabled');
        $('#fast3, #fast4').click(function () {
            $(this).val('').attr('placeholder', 'Upgrade required');
            $('#premium-modal').modal('show');
        });
        
        $('#submit-fast-search').click(function () {
