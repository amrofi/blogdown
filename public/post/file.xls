    }

    function mpbLoadedCallback(wrap_id) {
        $('.more-btn').addClass('show-btn');
        if ($(window).width() < 550) {
            $(wrap_id).hide();
        }
        $('.more-btn .btn').on('click', function () {
            var current = $(this).data('display');

            if (current == 'up') {
                $(wrap_id).show();
                $(this).find('i').removeClass('fa-angle-double-down');
                $(this).find('i').addClass('fa-angle-double-up');
                $(this).data('display', 'down');
            } else {
                $(wrap_id).hide();
                $(this).find('i').addClass('fa-angle-double-down');
                $(this).find('i').removeClass('fa-angle-double-up');
                $(this).data('display', 'up');
            }
        });
    }

    function getStatsFeed(type) {
        var position_stats = {};
        position_stats['stats-total'] = {};
        position_stats['stats-avg'] = {};
        var position_id = 'QB';
        var categories = JSON.parse('{"points":"stats-pts-col","pass_cmp":"","pass_att":"","pass_pct":"","pass_yds":"","pass_tds":"","pass_ints":"","rush_att":"stats-col","rush_yds":"","rush_tds":"","pass_sacks":""}');
        var exclude_avg = ['pass_pct'];
        var custom_points = JSON.parse('{}');

        var stats_url = 'https://partners.fantasypros.com/api/v1/nfl-stats.php?week=20&year=2019&position=' + position_id;
        $.ajax({
            url: stats_url,
            dataType: 'JSON',
            type: 'GET',
            success: function (data) {
                var players = data.players;
                if (typeof players === 'object' && players.length > 0) {
                    var player_count = players.length;
                    for (var i = 0; i < player_count; i++) {
                        var player = players[i];
                        if (player.stats) {
                            var stats = {};
                            var stats_avg = {};
                            for (var cat in categories) {
                                if (cat == 'points' && Object.keys(custom_points).length > 0) {
                                    if (custom_points[player.player_id] !== undefined) {
                                        stats[cat] = custom_points[player.player_id];
                                    } else {
                                        stats[cat] = 0;
                                    }
                                } else if (cat == 'pass_pct' && player.stats['pass_cmp'] !== undefined && player.stats['pass_att'] !== undefined) {
                                    stats['pass_pct'] = Math.round((player.stats['pass_cmp'] / player.stats['pass_att']) * 100) + '%';
                                } else if (cat == 'def_td' && player.stats['def_fum_td'] !== undefined && player.stats['def_int_td'] !== undefined) {
                                    stats['def_td'] = player.stats['def_fum_td'] + player.stats['def_int_td'];
                                } else if (cat == 'sp_td' && player.stats['kick_ret_td'] !== undefined && player.stats['punt_ret_td'] !== undefined) {
                                    stats['sp_td'] = player.stats['kick_ret_td'] + player.stats['punt_ret_td'];
                                } else if (player.stats[cat] !== undefined) {
                                    stats[cat] = player.stats[cat];
                                } else {
                                    stats[cat] = 0;
                                }

                                if (exclude_avg.indexOf(cat) === -1) {
                                    if (player.stats['games'] !== undefined && player.stats['games'] !== 0) {
                                        stats_avg[cat] = (stats[cat] / player.stats['games']).toFixed(1);
                                    } else {
                                        stats_avg[cat] = '-';
                                    }
                                } else {
                                    stats_avg[cat] = stats[cat];
                                }

                            }

                            // Add check for fantasy points from JS feed
                            position_stats['stats-total'][player.player_id] = stats;
                            position_stats['stats-avg'][player.player_id] = stats_avg;
                        }
                    }
                }
                existing_views['stats-total'] = position_stats['stats-total'];
                existing_views['stats-avg'] = position_stats['stats-avg']
                createStats(position_stats[type], categories, type);
            },
            error: function (e) {
                console.log('Failed to retrieve stats');
            }
        });
    }

    function createStats(stats, categories, type) {
        var stats = stats || {};

        var table = $('#stats-table-wrapper tbody').empty();
        $('#rankings-table-wrapper .player-table .player-row').each(function () {
            var row = $(this).clone();
            $('td', row).slice(3).remove();
            var player_id = row.data('id');

            if (stats[player_id]) {
                for (cat in stats[player_id]) {
                    row.append('<td class="' + categories[cat] + '">' + stats[player_id][cat] + '</td>');
                }
            } else {
                var no_stat = (type == 'stats-avg') ? '0.0' : '0';
                for (cat in categories) {
                    row.append('<td class="' + categories[cat] + '">' + no_stat + '</td>');
                }
            }
            table.append(row);
        });

        $('.table-view-wrapper').hide();
        $('#stats-table-wrapper').show();
        $('#stats-data').trigger('update');
    }

    function getNotesFeed() {
        var all_notes = {};
                var experts = ['150','769','1136','1169','261','1138','678'];
        var waiver_notes_url = 'https://partners.fantasypros.com/api/v1/notes.php?cors=1&sport=NFL&week=17&type=WAIVER';
        $.ajax({
            url: waiver_notes_url,
            dataType: 'JSON',
            type: 'GET',
            success: function (data) {
                var notes = data.notes;
                if (typeof notes === 'object' && notes.length > 0) {
                    for (var i = 0; i < notes.length; i++) {
                        var player_notes = notes[i];
                        var note_len = player_notes.notes.length;
                        for (var j = 0; j < note_len; j++) {
                            if (experts.indexOf(player_notes.notes[j].expert_id) !== -1) {
                                all_notes[player_notes.player_id] = player_notes.notes[j].content;
                                break;
                            }
                        }
                    }
                }
                getNotesByType(all_notes);
            },
            error: function (e) {
                getNotesByType();
            }
        });
            }

    function getNotesByType(notes) {
        var all_notes = notes || {};

                var notes_url = 'https://partners.fantasypros.com/api/v1/notes.php?cors=1&sport=NFL&type=WEEKLY&week=21';
        $.ajax({
            url: notes_url,
            dataType: 'JSON',
            type: 'GET',
            success: function (data) {
                var notes = data.notes;
                if (typeof notes === 'object' && notes.length > 0) {
                    var experts = ['150','769','1136','1169','261','1138','678'];
                    for (var i = 0; i < notes.length; i++) {
                        var player_notes = notes[i];
                        // skip if we already have a waiver note
                        if (!all_notes[player_notes.player_id]) {
                            var note_len = player_notes.notes.length;
                            for (var j = 0; j < note_len; j++) {
                                if (experts.indexOf(player_notes.notes[j].expert_id) !== -1) {
                                    all_notes[player_notes.player_id] = player_notes.notes[j].content;
                                    break;
                                }
                            }
                        }
                    }
                }
                createNotes(all_notes);
            },
            error: function (e) {
                createNotes(all_notes);
            }
        });
            }

    function createNotes(notes) {
        var notes = notes || {};
        existing_views['notes'] = notes;

        $('#rankings-table-wrapper .player-table .player-row').each(function () {
            var row = $(this);
            var player_id = row.data('id');
            row.append('<td class="view-options notes" style="text-align:left"><div class="note-wrap">' + (notes[player_id] ? convert_smart_quotes(notes[player_id]) : '&nbsp') + '</div></td>');
        });

        $('.view-options').hide();
        $('.notes').show();
        $('.table-view-wrapper').hide();
        $('#rankings-table-wrapper').show();
        $('#rank-data').show();
        $('#rank-data').trigger('update');
    }

    function convert_smart_quotes(a) {
        return a.replace(/'\b/g, "\u2018")  // Opening singles
            .replace(/\b'/g, "\u2019")  // Closing singles
            .replace(/"\b/g, "\u201c")  // Opening doubles
            .replace(/\b"/g, "\u201d")  // Closing doubles
            .replace(/--/g, "\u2014"); // em-dashes
    };
</script>

