    function wsisCompare(players, extra_slots, cta) {
        var count = players.length;
        var params = "";
        var wsis = $('.wsis-view');

        $('.player-column').remove();
        for (var i = 0; i < count; i++) {
            var player = players[i];
            console.log(player);
            var html = $('<div class="wsis-column player-column">');
            html.append('<a class="player-name" href="/nfl/players/' + $(player).data('file') + '" target="_blank"><strong><span class="full-name">' + $(player).data('name') + '</span><span class="short-name">' + $(player).data('shortname') + '</span></strong></a>');
            html.append('<div class="player-team">' + $(player).data('position') + ' - ' + $(player).data('team') + '</div>');
            if ($(player).data('position') != 'DST') {
                html.append('<div class="player-photo"><a href="/nfl/players/' + $(player).data('file') + '" target="_blank"><img src="//images.fantasypros.com/images/nfl/players/210x210/' + $(player).data('id') + '.jpg" alt="' + $(player).data('name') + '" onError="this.src=\'//images.fantasypros.com/images/photo_missing_square.jpg\';"/></a></div>');
            } else {
                html.append('<div class="player-photo"><a href="/nfl/players/' + $(player).data('file') + '" target="_blank"><img src="//images.fantasypros.com/images/photo_missing_square.jpg" alt="' + $(player).data('name') + '"/></a></div>');
            }
            html.append('<div class="player-matchup">' + ($(player).data('opp') ? $(player).data('opp') : '') + ($(player).data('kickoff') ? ' <span class="hidden-phone">, </span><span class="game-time">' + $(player).data('kickoff') + '</span>' : '') + '</div>');

            wsis.before(html);
        }

        if (cta) {
            var html = $('<div class="wsis-column player-column">');
            html.append('<a class="player-name disable-link"><strong><span>Select</span></strong></a>');
            html.append('<div class="player-team">Another Player</div>');
            html.append('<div class="player-photo"><a class="cta" href="#premium-modal"><img src="//images.fantasypros.com/images/photo_missing_square.jpg"></a></div>');
            html.append('<div class="player-matchup"><a class="wsis-upgrade" href="https://secure.fantasypros.com/plans/?sport=nfl&type=core-fp">Upgrade to compare 3-4 players</a></div>');
            //html.append('<br><img style="width: 14px; height: 16px; margin-top: 5px;" src="https://images.fantasypros.com/images/icons/gray_lock_42x48.png">');
            wsis.before(html);
        } else {
            var i = 0;
            while (i < extra_slots) {
                var html = $('<div class="wsis-column player-column">');
                html.append('<a class="player-name disable-link"><strong><span>Select</span></strong></a>');
                html.append('<div class="player-team">Another Player</div>');
                html.append('<div class="player-photo"><a><img src="//images.fantasypros.com/images/photo_missing_square.jpg"></a></div>');
                //html.append('<div class="player-matchup">Use the WSIS checkboxes</div>');
                wsis.before(html);
                i++;
            }
        }

        if (count == 2) {
            var player_one = players[0].dataset['file'].replace('.php', '-');
            var player_two = players[1].dataset['file'];
            $('.wsis-link').prop('href', '/nfl/start/' + player_one + player_two + params);
        } else if (count > 2) {
            var filters = [];
            for (var i = 0; i < count; i++) {
                var player_id = players[i].dataset['id'];
                filters.push('filter[]=' + player_id);
            }
            $('.wsis-link').prop('href', '/nfl/start/?' + filters.join('&') + "");
        }
        setWsisContainer();
    }

    function setWsisContainer(initial) {
        var w = window.innerWidth;
        var columns = $('.wsis-column').length;
        var spacing = 10;
        var left = spacing;
        var width = $('.main-content').innerWidth() - (2 * spacing);

        if (w >= 1200) {
            left = 240 + spacing;
            if (columns <= 3) {
                left += 135 + spacing;
                width -= 270 + (2 * spacing);
            } else if (columns == 4) {
                left += 80 + spacing;
                width -= 160 + (2 * spacing);
            }
        } else if (w >= 900) {
            left = 240 + spacing;
            width = $('.inner').innerWidth() - (2 * spacing);
            if (columns <= 3) {
                left += 115 + spacing;
                width -= 230 + (2 * spacing);
            } else if (columns == 4) {
                left += 65 + spacing;
                width -= 130 + (2 * spacing);
            }
        }

        if (w >= 550) {
            $('.wsis-column').css('width', (100 / columns) + '%');
        } else {
            $('.player-column').css('width', (100 / (columns - 1)) + '%');
            $('.wsis-view').css('width', '100%');
        }

        $('#wsis-module').css({'margin-left': left + 'px', 'width': width + 'px'});
    }

    function initTableView(type) {
        updatePrintLink(type);
